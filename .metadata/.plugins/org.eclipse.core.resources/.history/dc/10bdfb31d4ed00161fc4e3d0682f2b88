package workshop3;

public class StudentTest {

	public static void main(String[] args) {
	 Student studentArray [] =new Student[3];
	 
	 Student s = new Student();
	 s.setName("홍길동");
	 s.setAge(15);
	 s.setHeight(171);
	 s.setWeight(81);
	 
	 Student  s2= new Student();
	 s2.setName("한사람");
	 s2.setAge(13);
	 s2.setHeight(183);
	 s2.setWeight(72);
	 
	 Student s3= new Student();
